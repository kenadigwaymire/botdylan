<robot name="guitar_neck">
  <!-- Base Link (Guitar Neck) -->
  <link name="world"/>

  <material name="brown"> <color rgba="138 80 44 0.8"/> </material>

  <joint name="neck_to_world" type="fixed">
    <parent link="world"/>
    <child link="neck"/>
    <origin xyz="0.00 0.00 0.00" rpy="0 0 0"/>
  </joint>

  <link name="neck">
    <visual>
      <origin xyz="0.00 0.00 0.00" rpy="0 0 0"/>
      <geometry> <box size="0.50 0.60 0.05"/> </geometry>
      <material name="orange"/>
    </visual>
    <collision>
      <origin          xyz="0.00 0.00 0.00" rpy="0 0 0"/>
      <geometry> <box size="0.50 0.60 0.05"/> </geometry>
    </collision>
  </link>
</robot>
<?xml version="1.0" ?>

<robot name="guitar_neck">

  <link name="world"/>

  <material name="brown"> <color rgba="0.67 0.49 0.34 1"/> </material>
  <material name="silver"> <color rgba="0.89 0.89 0.89 1"/> </material>

  <joint name="neck_to_world" type="fixed">
    <parent link="world"/>
    <child link="neck"/>
    <origin xyz="0.00 0.05 0.25" rpy="1.57 1.57 0"/>
  </joint>

  <link name="neck">
    <visual>
      <origin xyz="0.00 0.00 0.00" rpy="0 0 0"/>
      <geometry> <box size="0.10 1.5 0.02"/> </geometry>
      <material name="brown"/>
    </visual>
    <collision>
      <origin          xyz="0.00 0.00 0.00" rpy="0 0 0"/>
      <geometry> <box size="0.10 1.5 0.02"/> </geometry>
    </collision>
  </link>

  <joint name="string1_to_neck" type="fixed">
    <parent link="neck"/>
    <child link="str_high_e"/>
    <origin xyz="0.0375 0.00 0.005" rpy="0 0 0"/>
  </joint>

  <link name="str_high_e">
    <visual>
        <origin xyz="0.00 0.00 0.005" rpy="1.57 0 0"/>
        <geometry> <cylinder radius="0.002" length="1.5"/> </geometry>
        <material name="silver"/>
    </visual>
    <collision>
        <origin xyz="0.00 0.00 0.005" rpy="1.57 0 0"/>
        <geometry> <cylinder radius="0.002" length="1.5"/> </geometry>
    </collision>
  </link>

  <joint name="high_e_to_b" type="fixed">
    <parent link="str_high_e"/>
    <child link="str_b"/>
    <origin xyz="0.0 0.0 0.00" rpy="0 0 0"/>
  </joint>

  <link name="str_b">
    <visual>
        <origin xyz="0.00 -0.0125 0.000" rpy="1.57 0 0"/>
        <geometry> <cylinder radius="0.002" length="1.5"/> </geometry>
        <material name="silver"/>
    </visual>
    <collision>
        <origin xyz="0.00 0.00 0.005" rpy="1.57 0 0"/>
        <geometry> <cylinder radius="0.002" length="1.5"/> </geometry>
    </collision>
  </link>

</robot>